{
    "name": "joomla/string",
    "type": "joomla-package",
    "description": "Joomla String Package",
    "keywords": ["joomla", "framework", "string"],
    "homepage": "https://github.com/joomla-framework/string",
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.3.10"
    },
    "require-dev": {
        "joomla/test": "~1.0",
        "phpunit/phpunit": "4.*",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\String\\": "src/",
            "Joomla\\String\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
